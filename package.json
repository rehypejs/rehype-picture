{
  "name": "rehype-picture",
  "version": "2.0.0",
  "description": "rehype plugin to wrap images in pictures",
  "license": "MIT",
  "keywords": [
    "unified",
    "rehype",
    "rehype-plugin",
    "hast",
    "html",
    "img",
    "image",
    "figure",
    "picture"
  ],
  "repository": "rehypejs/rehype-picture",
  "bugs": "https://github.com/rehypejs/rehype-picture/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "hast-util-is-element": "^1.0.0",
    "replace-ext": "^1.0.0",
    "unist-util-visit": "^1.1.1"
  },
  "devDependencies": {
    "browserify": "^16.0.0",
    "nyc": "^14.0.0",
    "prettier": "^1.0.0",
    "rehype-parse": "^6.0.0",
    "rehype-stringify": "^6.0.0",
    "remark-cli": "^6.0.0",
    "remark-preset-wooorm": "^5.0.0",
    "tape": "^4.0.0",
    "tinyify": "^2.0.0",
    "unified": "^8.0.0",
    "xo": "^0.24.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "build-bundle": "browserify . -s rehypePicture > rehype-picture.js",
    "build-mangle": "browserify . -s rehypePicture -p tinyify > rehype-picture.min.js",
    "build": "npm run build-bundle && npm run build-mangle",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run format && npm run build && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "guard-for-in": "off"
    },
    "ignores": [
      "rehype-picture.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
